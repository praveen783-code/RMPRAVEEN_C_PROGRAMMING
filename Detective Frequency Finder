
#include <stdio.h>

int main() {

    int n, x, count = 0;

    

    // Read the number of elements

    scanf("%d", &n);

    

    int arr[n];

    

    // Read the array elements

    for (int i = 0; i < n; i++) {

        scanf("%d", &arr[i]);

    }

    

    // Read the element to find frequency of

    scanf("%d", &x);

    

    // Count frequency of x

    for (int i = 0; i < n; i++) {

        if (arr[i] == x) {

            count++;

        }

    }

    

    // Print the frequency

    printf("%d\n", count);

    

    return 0;

}
