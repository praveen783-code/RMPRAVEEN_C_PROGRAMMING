#include <stdio.h>

int main() {

    int matrix[3][3];

    int sum = 0;

    

    // Read the 3x3 matrix and calculate sum

    for(int i = 0; i < 3; i++) {

        for(int j = 0; j < 3; j++) {

            scanf("%d", &matrix[i][j]);

            sum += matrix[i][j];

        }

    }

    

    // Print the sum

    printf("%d\n", sum);

    

    return 0;

}
