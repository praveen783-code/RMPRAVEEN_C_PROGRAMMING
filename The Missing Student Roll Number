#include <stdio.h>

int main() {

    int n;

    scanf("%d", &n);

    

    int arr[n - 1];

    int total_sum = n * (n + 1) / 2; // sum of 1 to n

    int actual_sum = 0;

    

    for (int i = 0; i < n - 1; i++) {

        scanf("%d", &arr[i]);

        actual_sum += arr[i];

    }

    

    int missing = total_sum - actual_sum;

    printf("%d\n", missing);

    

    return 0;

}
