#include <stdio.h>

int main()

{

    int n, i;

    float arr[20], max;

    scanf("%d", &n);

    for(i = 0; i < n; i++)

    {

        scanf("%f", &arr[i]);

    }

    max = arr[0];

    for(i = 1; i < n; i++)

    {

        if(arr[i] > max)

        {

            max = arr[i];

        }

    }

    printf("%.2f", max);

    return 0;

}
