#include <stdio.h>

int main()
{
    int n, i;
    int arr[20];
    int min;

    scanf("%d", &n);

    for(i = 0; i < n; i++)
    {
        scanf("%d", &arr[i]);
    }

    min = arr[0];   // assume first element is smallest

    for(i = 1; i < n; i++)
    {
        if(arr[i] < min)
        {
            min = arr[i];
        }
    }

    printf("%d", min);

    return 0;
}
