#include <stdio.h>

int main() {
    int n, i;
    scanf("%d", &n);

    int arr[n];
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    int sum1 = 0, sum2 = 0;

    // First half sum
    for(i = 0; i < n/2; i++) {
        sum1 += arr[i];
    }

    // Second half sum
    for(i = n/2; i < n; i++) {
        sum2 += arr[i];
    }

    if(sum1 == sum2)
        printf("BALANCED");
    else
        printf("NOT BALANCED");

    return 0;
}
