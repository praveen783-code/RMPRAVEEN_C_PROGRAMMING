#include <stdio.h>

int main() {

    int matrix1[3][3], matrix2[3][3], result[3][3];

    

    // Read first matrix

    for(int i = 0; i < 3; i++) {

        for(int j = 0; j < 3; j++) {

            scanf("%d", &matrix1[i][j]);

        }

    }

    

    // Read second matrix

    for(int i = 0; i < 3; i++) {

        for(int j = 0; j < 3; j++) {

            scanf("%d", &matrix2[i][j]);

        }

    }

    

    // Subtract matrices (first - second)

    for(int i = 0; i < 3; i++) {

        for(int j = 0; j < 3; j++) {

            result[i][j] = matrix1[i][j] - matrix2[i][j];

        }

    }

    

    // Print result

    for(int i = 0; i < 3; i++) {

        for(int j = 0; j < 3; j++) {

            printf("%d ", result[i][j]);

        }

        printf("\n");

    }

    

    return 0;

}
